{
  "name": "oracledb",
  "version": "1.10.1",
  "description": "Oracle Database driver by Oracle Corp.",
  "license": "Apache-2.0",
  "homepage": "http://www.oracle.com/technetwork/database/database-technologies/scripting-languages/node_js/",
  "keywords": [
    "oracledb",
    "dboracle",
    "Node.js",
    "SQL",
    "PL/SQL",
    "connection",
    "connectivity",
    "OCI",
    "client",
    "plugin",
    "library",
    "driver",
    "add-on",
    "extension",
    "binding",
    "interface",
    "adapter",
    "module",
    "DB",
    "official",
    "Database",
    "Oracle"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/oracle/node-oracledb.git"
  },
  "dependencies": {
    "node-pre-gyp": "0.6.x",
    "nan": "~2.3.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
      "mocha": "^2.4.5",
      "should": "^8.3.1",
      "async": "^1.5.0"
  },
  "scripts": {
      "install": "node-pre-gyp install --fallback-to-build",
      "test": "mocha --opts test/opts/mocha.opts",
      "testWindows": "mocha --opts test\\opts\\mocha.opts"
  },
  "binary": {
      "module_name": "oracledb",
      "module_path": "./build/Release",
      "host": "https://raw.githubusercontent.com/bchr02/node-oracledb/prebuild_support/bin",
      "remote_path": "",
      "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}-{toolset}.tar.gz"
},
  "engines": {
    "node": ">=0.10.28 <7"
  },
  "maintainers": [
